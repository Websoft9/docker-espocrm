{
  "name": "template",
  "trademark": "ONLYOFFICE",
  "installpath": "/data/wwwroot/espocrm",
  "compose_file": "docker-compose.yml",
  "fork_url": "https://github.com/espocrm/espocrm-docker",
  "env_file": ".env",
  "requirements":{
    "cpu_arch": "Linux x86-64, ARM 32/64, Windows x86-64, IBM POWER8, x86/i686",
    "cpu": "1",
    "memory": "1",
    "disk": "20",
    "swap": "2",
    "url": "https://docs.espocrm.com/administration/server-configuration/"
  },

  "user": {
    "username": "admin",
    "password": "password"
  },

  "services":[
    {
      "name": "espocrm",
      "port": "9001",
      "description_cn": "浏览器访问 EspoCRM",
      "description_en": "Access to EspoCRM by browser",
      "required": "Y"
    }
  ],

  "command_comments":[
    {
      "comment_cn": ".env文件的SITE_NAME需要修改成公网IP或者域名才能被外网访问",
      "comment_en": ".env file's [SITE_NAME] should  be changed to public IP or domain name to be accessed by Internet"
    }
  ]

}
